<UserControl x:Class="RingPlayer24.AutomatedRingPlayer24"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RingPlayer24"
             xmlns:themes="clr-namespace:PlayerControls.Themes;assembly=PlayerControls"
             xmlns:sys="clr-namespace:RingPlayer24._sys"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<UserControl.ContextMenu>
		<ContextMenu>
			<MenuItem Header="Close" Click="CloseClicked"></MenuItem>
		</ContextMenu>
	</UserControl.ContextMenu>
	<Grid>
		<themes:ScheduledFramePresenter ItemsSource="{Binding Source={x:Static sys:RingManager.I}, Path=ScheduledFrames}" />
	</Grid>
</UserControl>
